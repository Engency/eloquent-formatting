{
  "name": "engency/eloquent-formatting",
  "description": "Eloquent Custom Formatting",
  "keywords": [
    "eloquent",
    "model",
    "data-structure",
    "laravel",
    "formatting"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Engency",
      "homepage": "https://www.engency.com"
    },
    {
      "name": "Frank Kuipers",
      "email": "frank@engency.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/database": "^7.0|^8.0",
    "nesbot/carbon": "^2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Engency\\DataStructures\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },
  "scripts": {
    "check-style": "phpcs -p --standard=psr2 src/",
    "fix-style": "phpcbf -p --standard=psr2 src/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
